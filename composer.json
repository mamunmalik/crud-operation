{
    "name": "mamunmalik/crud-operation",
    "description": "Laravel package to generate a complete CRUD operation",
    "keywords": [
        "mamunmalik",
        "crud-operation"
    ],
    "homepage": "https://github.com/mamunmalik/crud-operation",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Mamun Malik",
            "email": "mamun5566@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Mamunmalik\\CrudOperation\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mamunmalik\\CrudOperation\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mamunmalik\\CrudOperation\\CrudOperationServiceProvider"
            ],
            "aliases": {
                "CrudOperation": "Mamunmalik\\CrudOperation\\CrudOperationServiceProvider"
            }
        }
    }
}